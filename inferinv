#!/bin/bash --posix
SCALACLASSPATH="$LEON_HOME/src/main/resources:$LEON_HOME/target/scala-2.10/classes:$LEON_HOME/unmanaged/64/cafebabe_2.10-1.2.jar:$LEON_HOME/unmanaged/64/insynth_2.10-2.1.jar:$LEON_HOME/unmanaged/64/scala-smt-lib_2.10-0.1.jar:$LEON_HOME/unmanaged/64/scalaz3-osx-64b-2.1.1.jar:$LEON_HOME/unmanaged/64/vanuatoo_2.10-0.1.jar:/Users/Amir/.sbt/boot/scala-2.10.2/lib/scala-library.jar:/Users/Amir/.sbt/boot/scala-2.10.2/lib/scala-compiler.jar:/Users/Amir/.ivy2/cache/org.scala-lang/scala-reflect/jars/scala-reflect-2.10.2.jar:/Users/Amir/.ivy2/cache/com.typesafe.akka/akka-actor_2.10/jars/akka-actor_2.10-2.2.0.jar:/Users/Amir/.ivy2/cache/com.typesafe/config/bundles/config-1.0.2.jar"

source $LEON_HOME/./setupenv
java -Xmx2G -Xms512M -classpath ${SCALACLASSPATH} -Dscala.home="$SCALA_HOME" -Dscala.usejavacp=false scala.tools.nsc.MainGenericRunner -classpath ${SCALACLASSPATH} leon.Main --inferInv $@ 2>&1 | tee last.log